<div class="page-header">
  <h1>Index#index</h1>
</div>
<p>Find me in app/views/index/index.html.erb</p>

<% User.all.each do |user| %>
	<table>
			<tr>
				<td >
					<a class="pull-left" href="<%= user_path(user.id) %>">
		                <img class="media-object dp img-circle" src="<%= user.avatar.url(:thumb) %>" style="width: 50px;height:50px;">
		            </a>
		        </td>
				<td>
					<h4>&nbsp;&nbsp;<%=link_to user.nickname, user %></h4>
				</td>
			</tr>
			
		</table>
<%end%>
